"""
###String-2: cat_dog###
def cat_dog(str):
  count_dog = 0
  count_cat = 0
  
  for i in range(len(str)-2):
    if str[i:i+3] == "dog":
      count_dog = count_dog + 1
    if str[i:i+3] == "cat":
      count_cat = count_cat + 1  
        
  if count_dog == count_cat:
    return True
  return False
"""
    
"""
###String-2: count_code###
def count_code(str):
  count = 0
  for i in range(len(str)-3):
    if str[i:i+2] == "co" and str[i+3] == "e":
        count = count + 1
  return count
